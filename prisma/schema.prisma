generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model webhooks {
  id                                   String     @id @db.VarChar
  name                                 String     @db.VarChar
  author                               String     @db.VarChar
  secret                               String     @db.VarChar
  created_at                           DateTime   @default(now()) @db.Timestamptz(6)
  messages_messages_webhooksTowebhooks messages[] @relation("messages_webhooksTowebhooks")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model messages {
  id                                   BigInt   @id @default(autoincrement())
  created_at                           DateTime @default(now()) @db.Timestamptz(6)
  webhooks                             String   @db.VarChar
  content                              String
  webhooks_messages_webhooksTowebhooks webhooks @relation("messages_webhooksTowebhooks", fields: [webhooks], references: [id], onDelete: Cascade, onUpdate: NoAction)
}
